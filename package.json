{
  "name": "@pauloelias/gatsby-tailwind-emotion-starter",
  "description": "A bare-bones Tailwind CSS + Emotion starter for Gatsby",
  "version": "2.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ext ts --ext tsx'",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "gatsby": "^2.23.1",
    "gatsby-image": "^2.4.15",
    "gatsby-plugin-emotion": "^4.3.3",
    "gatsby-plugin-postcss": "^2.3.3",
    "gatsby-plugin-manifest": "^2.4.22",
    "gatsby-plugin-offline": "^3.2.23",
    "gatsby-plugin-react-helmet": "^3.3.3",
    "gatsby-plugin-sharp": "^2.6.26",
    "gatsby-plugin-typescript": "^2.4.18",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-sharp": "^2.5.12",
    "postcss-import": "^12.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "typecript": "^0.0.1-security"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "babel-preset-gatsby": "^0.2.26",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.1.6",
    "twin.macro": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pauloelias/gatsby-tailwind-emotion-starter"
  },
  "bugs": {
    "url": "https://github.com/pauloelias/gatsby-tailwind-emotion-starter/issues"
  }
}
